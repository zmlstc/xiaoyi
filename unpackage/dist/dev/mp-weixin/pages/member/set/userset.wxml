<view><view data-event-opts="{{[['tap',[['upload',['$event']]]]]}}" class="pic-column mb2 mt10" bindtap="__e"><view class="item">头像</view><view class="itme2"><view class="icon"></view><image class="avatar" src="{{memberInfo.avatar}}"></image></view></view><view data-event-opts="{{[['tap',[['toPage',['editnick']]]]]}}" class="set-column mb2" bindtap="__e"><view class="item">昵称</view><view class="itme2">{{memberInfo.nickname}}<view class="icon"></view></view></view><view data-event-opts="{{[['tap',[['toPage',['realverify']]]]]}}" class="set-column mb2" bindtap="__e"><view class="item">实名认证</view><view class="itme2"><view class="icon"></view><block wx:if="{{is_realverify==='1'}}"><view class="finish">已认证</view></block><block wx:if="{{is_realverify==='0'}}"><view class="no-finish">未认证</view></block><block wx:if="{{is_realverify==='2'}}"><view class="no-finish">认证中</view></block></view></view></view>