.welcome{height:92rpx; text-align:right;width:90%;padding:0 5%;}
.welcome .title{line-height:56rpx;font-size:38rpx;}
.welcome .text{line-height:36rpx;color:#999;}
.agree {height:48rpx;width:80%;padding:0 10%;font-size:24rpx;color: #999;}
.agree text{color: #00BAAD;}
.nav-reg {width:90%;margin:0 5%;height:98rpx; font-size:34rpx;}
/*高度48px栏目条*/
.nav-reg input{width:100%;height:98rpx;font-size:28rpx;color:#666;}
.nav-reg .tip{width:100%;line-height:36rpx;color:#ccc;}
.nav-reg .item{width:22%;line-height:98rpx;float:left;text-align: right;}
.nav-reg .item .acc{float:right;width:58rpx;height:98rpx;background: url('https://www.fhlego.com/static/login-0.png') center no-repeat;background-size:80%;}
.nav-reg .item .pass{float:right;width:58rpx;height:98rpx;background: url('https://www.fhlego.com/static/login-2.png') center no-repeat;background-size:80%;}
.nav-reg .item .phone{float:right;width:58rpx;height:98rpx;background:url('https://www.fhlego.com/static/login-1.png') center no-repeat;background-size:80%;}
.nav-reg .item .qr{float:right;width:58rpx;height:98rpx;background:url('https://www.fhlego.com/static/login-5.png') center no-repeat;background-size:80%;}
.nav-reg .item .yq{float:right;width:58rpx;height:98rpx;background:url('https://www.fhlego.com/static/login-4.png') center no-repeat;background-size:80%;}
.nav-reg .item .yz{float:right;width:58rpx;height:98rpx;background:url('https://www.fhlego.com/static/login-3.png') center no-repeat;background-size:80%;}
.nav-reg .item2{width:55%;line-height:98rpx;float:left;color:#999;font-size:30rpx;margin-left:20rpx;}
.nav-reg .item3{width:40%;line-height:98rpx;float:left;color:#999;font-size:30rpx;margin-left:20rpx;}
.nav-reg .item4{line-height:98rpx;float:left;padding:24rpx 0;}
.nav-reg .item4 .title{padding:6rpx 16rpx;background:#00BAAD;line-height:38rpx;float:left;overflow:hidden;font-size:24rpx;border-radius:30rpx;color: #FFFFFF;}
.nav-reg .item4 .again{padding:6rpx 16rpx;background:#CCCCCC;line-height:38rpx;float:left;overflow:hidden;font-size:24rpx;border-radius:30rpx;color: #FFFFFF;}
.nav-reg .item5{width:32%;line-height:98rpx;float:left;text-align: right;}
.nav-reg .item6{width:64%;line-height:98rpx;float:left;margin-left:20rpx;color:#999999;font-size:28rpx;}
.nav-reg .area{width:64%;height:98rpx;line-height:98rpx;float:left;margin-left:20rpx;color:#999999;font-size:28rpx;overflow: hidden;text-overflow: ellipsis;}
.nav-reg .item6 .text{float: right;font-size:26rpx;color: #00BAAD;}
.nav-reg .item6 .upload{float: left;font-size:48rpx;border:#CCCCCC 2rpx solid;height:48rpx;width:48rpx;margin: 25rpx 20rpx 0 0;line-height:38rpx;text-align: center;}
.header {width:161rpx;height:161rpx;background:rgba(63,205,235,1);box-shadow:0rpx 12rpx 13rpx 0rpx rgba(63,205,235,0.47);border-radius:50%;margin-top: 30rpx;margin-left: auto;margin-right: auto;}
.header image{width:161rpx;	height:161rpx;border-radius:50%;}
.wxname {width:100%;line-height:48rpx;text-align:center;font-size:38rpx;}
.tishi {width:100%;line-height:48rpx;text-align:center;}
.tishi text{color: #00CF38;}

