<view><view class="rebate-bar b-line"><view class="item"><text data-event-opts="{{[['tap',[['showtab',[0]]]]]}}" class="{{[showtype==0?'tab':'']}}" bindtap="__e">全部</text><text data-event-opts="{{[['tap',[['showtab',[1]]]]]}}" class="{{[showtype==1?'tab':'']}}" bindtap="__e">商家</text><text data-event-opts="{{[['tap',[['showtab',[2]]]]]}}" class="{{[showtype==2?'tab':'']}}" bindtap="__e">会员</text></view><view class="item2"><text>累计佣金：</text><text class="tab">{{tg_predepoit}}</text></view></view><view class="action-bar b-line" style="background-color:#F5F5F5;"><view class="sc-bar w50 mc"><input class="sc_area" type="text" value="{{keyword}}" placeholder="{{placeholder}}" data-event-opts="{{[['input',[['__set_model',['','keyword','$event',[]]]]]]}}" bindinput="__e"/><view data-event-opts="{{[['tap',[['submitSearch',['$event']]]]]}}" class="sc_submit" bindtap="__e">搜 索</view></view></view><view><view class="date-opt"><button data-event-opts="{{[['tap',[['onShowDatePicker',['range']]]]]}}" class="datebutton" bindtap="__e">选择日期范围</button><view class="range">{{range[0]+" 至 "+range[1]}}</view></view><mx-date-picker vue-id="76040080-1" show="{{showPicker}}" type="{{type}}" value="{{value}}" show-tips="{{true}}" begin-text="开始" end-text="结束" show-seconds="{{true}}" data-event-opts="{{[['^confirm',[['onSelected']]],['^cancel',[['onSelected']]]]}}" bind:confirm="__e" bind:cancel="__e" bind:__l="__l"></mx-date-picker></view><block wx:for="{{datalist}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="rebate-list"><view class="item"><block wx:if="{{item.lg_type=='store_invite'}}"><view class="shop"></view></block><block wx:if="{{item.lg_type=='member_invite'}}"><view class="mement"></view></block>{{''+item.show_text+''}}</view><view class="item2">{{item.lg_add_time_text}}</view><view class="item3"><text>￥</text>{{item.lg_av_amount}}</view></view></block><view class="loading-text">{{loadingText}}</view></view>