.catelist{width:94%;display: -webkit-box;display: -webkit-flex;display: flex;-webkit-flex-wrap: wrap;flex-wrap: wrap; box-sizing: border-box; -webkit-box-pack: justify; -webkit-justify-content: space-between; justify-content: space-between;}
.catelist .cate {-webkit-box-flex: 49%;-webkit-flex: 49% 0;flex: 49% 0;text-align:center;background:#fff;margin:20rpx 0rpx;border-radius: 11rpx;box-sizing: border-box;padding:20rpx 0;}
.catelist .cate .top {overflow:hidden;}
.catelist .cate .top .img{height:220rpx;overflow:hidden;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-align:center;-webkit-align-items:center;align-items:center;}
.catelist .cate .top .img .cateimg {width: 200rpx;height: 200rpx;}
.catelist .cate .top .title{line-height:32rpx;overflow:hidden;padding:10rpx 0;}
.catelist .cate .top .num{line-height:32rpx;color:#999;}
.catelist .cate .top .num text{color:#00BAAD;font-weight:bold;padding:0 5rpx;}
.catelist .cate .pic {overflow:hidden;}
.catelist .cate .pic .img{height:220rpx;overflow:hidden;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-align:center;-webkit-align-items:center;align-items:center;}
.catelist .cate .pic .img .photo {width:200rpx;height:200rpx;}
.catelist .cate .bottom {display: -webkit-box;display: -webkit-flex;display: flex;}
.catelist .cate .bottom .edit{-webkit-box-flex: 50%;-webkit-flex: 50% 0;flex: 50% 0;margin: 10rpx 20rpx;border: 1px solid #00867d;border-radius: 8rpx;background:#00BAAD;color:#fff;}
.catelist .cate .bottom .del {-webkit-box-flex: 50%;-webkit-flex: 50% 0;flex: 50% 0;margin: 10rpx 20rpx;border: 1px solid #ccc;border-radius: 8rpx;background:#eee;}
.upload{position: fixed;bottom: 0;width: 100%;border-radius: 0;}
.photolist{width:94%;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-flex-wrap:wrap;flex-wrap:wrap; box-sizing: border-box;-webkit-box-pack:justify;-webkit-justify-content:space-between;justify-content:space-between;}
.photolist .photo {-webkit-box-flex:48%;-webkit-flex:48% 0;flex:48% 0;text-align: center;background: #fff;margin:10rpx 0rpx;display:-webkit-box;display:-webkit-flex;display:flex;border-radius:10rpx;box-sizing: border-box;-webkit-box-align: center;-webkit-align-items: center;align-items: center;padding:20rpx;overflow:hidden;}
.photolist .photo .photoimg {width:200rpx;height:200rpx;}
page {
	background: #F5F5F5;
}

