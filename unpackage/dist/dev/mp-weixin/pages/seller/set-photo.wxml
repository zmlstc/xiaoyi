<view><view class="catelist mc"><block wx:for="{{cate}}" wx:for-item="item" wx:for-index="index"><view class="cate"><view data-event-opts="{{[['tap',[['toPage',['photolist?id='+item.ac_id]]]]]}}" class="top" bindtap="__e"><view class="img"><image class="cateimg mc" src="{{item.ac_cover}}" mode="widthFix"></image></view><view class="title">{{item.ac_name}}</view><view class="num">共<text>{{item.count}}</text>张</view></view><view class="bottom mt10"><view data-event-opts="{{[['tap',[['toPage',['editphotolist?id='+item.ac_id]]]]]}}" class="edit" bindtap="__e">编辑</view><view data-event-opts="{{[['tap',[['deletephotolist',['$0'],[[['cate','',index,'ac_id']]]]]]]}}" class="del" bindtap="__e">删除</view></view></view></block></view><view data-event-opts="{{[['tap',[['toPage',['newphotolist']]]]]}}" class="btn mt40 upload" bindtap="__e">新建相册</view></view>