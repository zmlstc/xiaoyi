<view><block wx:for="{{txwaylist}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="cash-mode bf b-line"><view class="item"><view class="{{[item.bill_type_code]}}"></view><view class="content"><block wx:if="{{item.bill_type_code=='wxpay'}}"><view class="name">微信提现</view></block><block wx:if="{{item.bill_type_code=='alipay'}}"><view class="name">支付宝账号</view></block><view class="acc">{{item.bill_type_number_txt}}</view></view></view><view class="item2"><block wx:if="{{item.bill_type_code=='alipay'}}"><view data-event-opts="{{[['tap',[['toPage',['alipay?id='+item.id]]]]]}}" class="edit" bindtap="__e"></view></block><block wx:if="{{item.bill_type_code=='wxpay'}}"><view data-event-opts="{{[['tap',[['toPage',['wxpay?id='+item.id]]]]]}}" class="edit" bindtap="__e"></view></block></view></view></block><block wx:if="{{have_wx==0}}"><view class="cash-mode bf b-line"><view class="item"><view class="wxpay"></view><view class="content"><view class="name">微信支付账号</view><view class="acc">请帮定您的微信提现账号</view></view></view><view class="item2"><view data-event-opts="{{[['tap',[['toPage',['wxpay']]]]]}}" class="add" bindtap="__e"></view></view></view></block><block wx:if="{{have_alipay==0}}"><view class="cash-mode bf b-line"><view class="item"><view class="alipay"></view><view class="content"><view class="name">支付宝账号</view><view class="acc">请帮定您的微信提现账号</view></view></view><view class="item2"><view data-event-opts="{{[['tap',[['toPage',['alipay']]]]]}}" class="add" bindtap="__e"></view></view></view></block></view>