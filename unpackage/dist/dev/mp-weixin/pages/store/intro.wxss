.column {width:90%;padding:0 5%;height:98rpx;font-size:28rpx;} /*高度48px栏目条*/
.column .item{width:20%;line-height:98rpx;float:left;}
.column .item2{width:75%;line-height:98rpx;float:right;color: #00BAAD;}
.column .item2 .text{width:88%;height:98rpx;line-height:98rpx;float:left;overflow: hidden;}
.column .item2 .icon{width:10%;height:98rpx;background: url('https://www.fhlego.com/static/arrow_r.png') center no-repeat; background-size:100%;float:right;}
.column .item3 {width:75%;height:98rpx;line-height:98rpx;float:right;color: #666;text-align: left;}
.column2{width:90%;padding:20rpx 5% 30rpx; overflow:hidden;} /*自适应高度样式*/
.column2 .title{width:100%;line-height:68rpx;}
.column2 .content{padding:8rpx 12rpx;line-height:40rpx;color:#999999;font-size:30rpx;}
.column2 .opt{width:13%;padding:8rpx 12rpx;line-height:40rpx;float:left;}
.column2 .opt .icon{width:80%;height:60rpx;background: url('https://www.fhlego.com/static/phone.png') center no-repeat; background-size:80%;}
.column2 .opt .name{font-size:30rpx;font-size: 26rpx;text-align: center;}
.column3 {width:100%;padding:20rpx 0;height:128rpx;} /*评价详情栏目条*/
.column3 .item{width:16%;padding:10rpx 20rpx;height:108rpx;text-align:center;float: left;}
.column3 .item .score{width:100%;line-height:58rpx;color: #00BAAD;font-size:54rpx;}
.column3 .item .score text{font-size:26rpx;}
.column3 .item .text{width:100%;line-height:32rpx;color:#FF6600;font-size:30rpx;}
.column3 .item2{width:78%;padding:10rpx 0;height:108rpx;float: right;}
.column3 .item2 .option{width:48%;padding:10rpx 4rpx;line-height:38rpx;font-size:30rpx;overflow: hidden;float: left;text-align: center;}
.column3 .item2 .option .name{width:22%;padding:0rpx 6rpx;float:left;}
.column3 .item2 .option .start{width:50%;height:38rpx;background: url('https://www.fhlego.com/static/start.png') center no-repeat; background-size:100%;float:left;}
.column3 .item2 .option .score{width:18%;padding:0rpx 6rpx;float:left;color:#00BAAD;}
.column4 {width:94%;padding:20rpx 3%;overflow: hidden;} /*高34px栏目条*/
.column4 .title{width:90%;padding-left:38rpx;line-height:48rpx;}
.column4 .content{width:95%;padding:10rpx 20rpx 10rpx 38rpx;line-height:32rpx;}
.column4 .item .title{padding:0 20rpx;line-height:104rpx;float:left; margin:0 10rpx;}
.facilities {width:94%;padding:20rpx 3%;overflow: hidden;}
.facilities .title{padding:0 20rpx 20rpx 38rpx;line-height:48rpx;}
.facilities .opt{width:95%;padding:0 20rpx 0 28rpx;overflow: hidden;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-flex-wrap:wrap;flex-wrap:wrap}
.facilities .opt .box{width:108rpx;height:84rpx;text-align:center;margin-bottom:40rpx;}
.facilities .opt .box .num{width:64rpx;height:64rpx;}
.facilities .opt .box .text{width: 100%;font-size:24rpx;line-height:32rpx;color: #666;margin-top: -10rpx;}
.column5 {width:90%;padding:0rpx 5%;height:208rpx;} /*高104px栏目条*/
.column5 .title{width:100%;padding:30rpx 0 10rpx;height:50rpx;line-height: 50rpx;text-align: center;font-size:38rpx;font-weight:500;}
.column5 .consume {width:100%;padding:10rpx 0;font-size:34rpx;font-weight:500;}
.column5 .consume input{width:90%;height:68rpx;line-height:68rpx;padding: 0 5%;}
.column6{width:90%;height:200rpx; margin:20rpx 5%; border-radius: 20rpx;} /*优惠券栏样式*/
.column6 .item{width:28%;height:160rpx; border-right: #CCCCCC 2rpx dashed;padding:20rpx 10rpx;text-align: center;float: left;}
.column6 .item .num{width:100%;line-height:68rpx;font-size:28rpx;}
.column6 .item .num text{font-size:54rpx;color: #FF6600;}
.column6 .item .tip{width:100%;line-height:38rpx;font-size:28rpx;}
.column6 .item .tip text{color: #00BAAD;}
.column6 .item2{width:65%;height:160rpx;padding:20rpx 10rpx;float:right;margin-left: 10rpx;}
.column6 .item2 .title{width:100%;height:46rpx;line-height:46rpx;margin-bottom:10rpx;overflow: hidden;}
.column6 .item2 .info {height:134rpx;overflow: hidden;}
.column6 .item2 .info .tip {width:85%;font-size:30rpx;color: #999999;line-height:36rpx;float:left;}
.column6 .item2 .info .select {width:15%;height:64rpx;background: url('https://www.fhlego.com/static/select.png') center no-repeat; background-size:80%;float:right;display:block;} /*评价详情样式*/
.reply-list{width:90%;padding:30rpx 5%;overflow:hidden;}
.reply-list .head{width:100%;padding:10rpx 0;line-height:88rpx;overflow: hidden;}
.reply-list .head .mement{width:50%;float:left;}
.reply-list .head .mement .avatar{width:88rpx;height:88rpx;background: url('https://www.fhlego.com/static/avatar.jpg') center no-repeat;background-size:100%;float:left;border-radius:50%;}
.reply-list .head .mement .sub{width:65%;height:88rpx;float:left;}
.reply-list .head .mement .sub .name{width:100%;line-height:48rpx;}
.reply-list .head .mement .sub .start{width:100%;height:40rpx;background: url('https://www.fhlego.com/static/start.png') left no-repeat; background-size:65%;}
.reply-list .head .reply{float:right;padding:0 10rpx;color:#00BAAD;}
.reply-list .head .date {float:right;color: #999999;font-size:26rpx;}
.reply-list .content{padding:30rpx 10rpx;line-height:40rpx;height:auto;color:#666666;}
.reply-list .reply{line-height:40rpx;overflow:hidden;padding:20rpx 10rpx 0;}
.reply-list .reply .answer{width:100%;line-height:42rpx;}
.reply-list .reply .answer .title{color:#00BAAD;}
.pic-navbar{width:90%;height:68rpx; padding:20rpx 5%;}
.pic-navbar .select{padding:10rpx 20rpx;line-height:68rpx;background: #00BAAD;color: #FFFFFF;border-radius:40rpx;}
.pic-navbar text{padding:10rpx;line-height:48rpx;width: auto;}
.pic-show {width:94%;padding:0 3%;overflow: hidden;}
.pic-show .item{width:46%;height:268rpx;float: left;padding:0 2%;}
.pic-show .item .album{width:100%;height:220rpx;border-radius: 20rpx;border: #EEEEEE 2rpx solid;}
.pic-show .item .pic{width:100%;height:220rpx;}
.pic-show .item .title{width:100%;line-height:48rpx;text-align: center;}

