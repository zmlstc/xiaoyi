<view><view class="b-line" style="width:60%;padding:20rpx 20%;background:#fff;"><view class="sc-bar"><input class="sc_area" type="text" value="{{keyword}}" placeholder="{{placeholder}}" data-event-opts="{{[['input',[['__set_model',['','keyword','$event',[]]]]]]}}" bindinput="__e"/><view data-event-opts="{{[['tap',[['submitSearch',['$event']]]]]}}" class="sc_submit" bindtap="__e">搜 索</view></view></view><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['toPage',['$0'],[[['list','',index,'store_id']]]]]]]}}" class="show-box row" bindtap="__e"><view data-event-opts="{{[['tap',[['deleteOrder',['$0'],[[['list','',index,'store_id']]]]]]]}}" class="menu" catchtap="__e"><view class="icon shanchu"></view></view><view data-event-opts="{{[['touchstart',[['touchStart',[index,'$event']]]],['touchmove',[['touchMove',[index,'$event']]]],['touchend',[['touchEnd',[index,'$event']]]]]}}" class="{{['carrier',theIndex==index?'open':oldIndex==index?'close':'']}}" bindtouchstart="__e" bindtouchmove="__e" bindtouchend="__e"><view class="box-left"><view class="image" style="{{'background:'+('url('+item.store_avatar_url+')')+';'+('background-size:'+('100%')+';')}}"></view><view class="text-box"><view class="title">{{item.store_name}}</view><view class="evaluate"><uni-rate vue-id="{{'1-'+index}}" disabled="true" size="14" value="{{item.store_score}}" bind:__l="__l"></uni-rate><view class="deal">已消费<text class="num">{{item.consume_num}}</text>人次</view></view></view></view><view class="box-right"></view></view></view></block><view class="loading-text">{{loadingText}}</view></view>