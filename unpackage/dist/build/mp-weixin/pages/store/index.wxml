<view><swiper class="carousel" indicator-dots="true" circular="true" interval="3000" duration="700"><block wx:for="{{storeInfo.plist}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item><view class="image-wrapper"><image class="{{[item.loaded]}}" src="{{item.img}}" mode="aspectFill"></image></view></swiper-item></block></swiper><view class="info"><view class="title"><text class="{{[(!loaded)?'Skeleton':'']}}">{{storeInfo.store_name}}</text></view><view><block wx:if="{{storeInfo.yy_state}}"><text class="iconfont icon-point-open"><text class="state">营业中</text></text></block><block wx:if="{{!storeInfo.yy_state}}"><text class="iconfont icon-point-close"><text class="state">休息中</text></text></block></view></view><view class="actions"><block wx:if="{{seval_scores>-1}}"><view class="actions-info"><uni-rate vue-id="1" disabled="true" size="14" margin="5" value="{{seval_scores}}" bind:__l="__l"></uni-rate></view></block><view class="actions-cost"><text>{{storeInfo.consume_num+"人消费"}}</text></view><view class="actions-coll"><block wx:if="{{!fav}}"><text data-event-opts="{{[['tap',[['toFavorite',['$event']]]]]}}" class="{{['iconfont icon-likefill',(fav)?'active':'']}}" bindtap="__e"></text></block><block wx:if="{{fav}}"><text data-event-opts="{{[['tap',[['toFavorite',['$event']]]]]}}" class="{{['iconfont icon-likefill',(fav)?'active':'']}}" bindtap="__e"></text></block></view></view><view class="store-contain"><view class="balance-info"><view class="left"><view data-event-opts="{{[['tap',[['toLocation',['$event']]]]]}}" class="left-top" bindtap="__e"><view class="icon"></view><view class="text clip">{{storeInfo.map_address}}</view><view class="iconfont icon-right"></view></view><view data-event-opts="{{[['tap',[['toPage',['intro?store_id='+store_id]]]]]}}" class="left-bottom" bindtap="__e"><view class="f-content"><block wx:for="{{storeInfo.flist}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="box"><image class="num _img" src="{{item.img}}"></image><view class="text">{{item.f_name}}</view></view></block></view><view class="iconfont icon-right fr"></view></view></view><view class="right"><view data-event-opts="{{[['tap',[['callPhone',['$0'],['storeInfo.store_phone']]]]]}}" class="box" bindtap="__e"><view class="img"><view class="iconfont icon-dianhua1"></view></view></view></view></view></view><view class="store-contain"><view class="balance-info"><view class="left"><block wx:if="{{storeInfo.vlist&&storeInfo.vlist.length>0}}"><view><view data-event-opts="{{[['tap',[['toggleMask',['show']]]]]}}" class="left-coupon ml10" bindtap="__e"><block wx:for="{{storeInfo.vlist}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="c-info"><view class="icon"></view><text>{{item.mtitle}}</text></view></block></view><view class="more"></view></view></block><block wx:else><view class="no-info"><view class="icon"></view><view class="text">商家暂无优惠券</view></view></block></view><view class="right"><view class="box"><view data-event-opts="{{[['tap',[['toPay',['$event']]]]]}}" class="buybtn" bindtap="__e">到店买单</view></view></view></view></view><view class="evalution"><view data-event-opts="{{[['tap',[['toPage',['evallist?store_id='+storeInfo.store_id]]]]]}}" class="evaluate-header" bindtap="__e"><view class="title"><text class="ml5">{{"用户评价 ("+storeInfo.eval_num+")"}}</text></view><block wx:if="{{storeInfo.eval_num>0}}"><view class="total"><text>{{storeInfo.eval_manyi+"%满意"}}</text><view class="icon"></view></view></block><block wx:if="{{storeInfo.eval_num==0}}"><view class="total">无评价<view class="icon"></view></view></block></view><view class="{{['eva-list',(!loaded)?'Skeleton':'']}}"><block wx:for="{{evaList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="eva-item b-line"><image class="img" src="{{item.member_avatar}}" mode="aspectFill"></image><view class="eva-right"><view class="acc"><view class="name">{{item.member_name}}</view><view class="start"><uni-rate vue-id="{{'2-'+index}}" disabled="true" size="14" margin="5" value="{{item.seval_scores}}" bind:__l="__l"></uni-rate></view><view class="time">{{item.date_txt+item.time_txt}}</view></view><text class="content">{{item.seval_content}}</text><block wx:if="{{item.geval_image_240}}"><view class="imgs"><block wx:for="{{item.geval_image_240}}" wx:for-item="imgurl" wx:for-index="key" wx:key="key"><block><view class="imgs-box"><image src="{{imgurl}}" mode="widthFix" data-event-opts="{{[['tap',[['previewImg',['$0','$1'],[[['evaList','',index],['geval_image_240','',key]],[['evaList','',index,'geval_image_240']]]]]]]}}" bindtap="__e"></image></view></block></block></view></block><block wx:if="{{item.is_reply=='1'&&item.reply_del=='0'}}"><view class="reply bg-color2"><view class="title">官方回复：</view><block wx:if="{{item.reply_del=='0'}}"><view class="content">{{item.seval_explain}}</view></block><view class="date"><view class="icon"></view><text>{{item.reply_date_txt}}</text><text class="ml5">{{item.reply_time_txt}}</text></view></view></block></view></view></block></view></view><view data-event-opts="{{[['tap',[['toggleMask',['$event']]]]]}}" class="{{['mask',maskState===0?'none':maskState===1?'show':'']}}" bindtap="__e"><view data-event-opts="{{[['tap',[['stopPrevent',['$event']]]]]}}" class="mask-content" catchtap="__e"><block wx:for="{{couponList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['selectcoupon',['$0'],[[['couponList','',index]]]]]]]}}" class="coupon-item" bindtap="__e"><view class="con"><view class="left"><text class="title">{{item.voucher_t_title}}</text></view><view class="right"><text class="price">{{item.voucher_t_price}}</text></view><view class="circle l"></view><view class="circle r"></view></view><view class="tip"><text class="time">{{"有效期至"+item.valid_time}}</text><text class="factor">{{"满"+item.voucher_t_limit+"可用"}}</text></view></view></block></view></view></view>