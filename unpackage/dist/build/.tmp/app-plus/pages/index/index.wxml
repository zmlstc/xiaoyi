<view bind:__l="__l"><view class="status"></view><view class="header"><view data-event-opts="{{[['tap',[['toCity',['$event']]]]]}}" class="addr" bindtap="__e"><view class="icon location"></view><text class="text">{{city}}</text></view><view class="input-box"><input placeholder="默认关键字" placeholder-style="color:#c0c0c0;" data-event-opts="{{[['tap',[['toSearch']]]]}}" bindtap="__e"/><view class="icon search"></view></view></view><view class="place"></view><view class="uni-tab-bar"><scroll-view class="uni-swiper-tab" id="tab-bar" scroll-x><block wx:for="{{tabClasslist}}" wx:for-item="tab" wx:for-index="index" wx:key="sc_id"><view class="{{['swiper-tab-list',tabIndex==index?'active':'']}}" id="{{tab.sc_id}}" data-current="{{index}}" data-event-opts="{{[['tap',[['tapTab',[index]]]]]}}" bindtap="__e">{{tab.sc_name}}</view></block></scroll-view></view><swiper class="swiper" previous-margin="20px" next-margin="20px" indicator-dots="{{false}}" autoplay="{{true}}" interval="{{5000}}" duration="{{1000}}" current="{{bigId}}" data-event-opts="{{[['change',[['swiperChange',['$event']]]]]}}" bindchange="__e"><block wx:for="{{bannerImg}}" wx:for-item="img" wx:for-index="index" wx:key="index"><swiper-item><view class="{{['swiper-item',bigId===index?'big':'small']}}"><image class="img" src="{{img.show_img}}"></image></view></swiper-item></block></swiper><block wx:if="{{navlist.length>0}}"><view class="nav-list"><block wx:for="{{navlist}}" wx:for-item="row" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['toCategory',['$0'],[[['navlist','',index,'url']]]]]]]}}" class="nav" bindtap="__e"><view class="img"><image src="{{row.img}}"></image></view><view class="text">{{row.name}}</view></view></block></view></block><view class="seckill-section m-t"><view class="s-header"><view class="title">会员福利</view><text class="other">换一批</text><text data-event-opts="{{[['tap',[['toUrl',['/pages/member/points/pshop']]]]]}}" class="more yticon icon-you" bindtap="__e">更多></text></view><scroll-view class="floor-list" scroll-x><view class="scoll-wrapper"><block wx:for="{{pgList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['toPage',['$0'],[[['pgList','',index,'pgoods_id']]]]]]]}}" class="floor-item" bindtap="__e"><image src="{{item.pgoods_image}}" mode="aspectFill"></image><view class="title clamp">{{item.pgoods_name}}</view><view class="price"><view class="icon"></view>{{item.pgoods_points}}</view><view class="clamp duihuan">{{"已兑换: "+item.pgoods_salenum}}</view></view></block></view></scroll-view></view><view class="guesslike-section"><view class="s-header"><view class="title">猜你喜欢</view></view><block wx:if="{{storelist.length>0}}"><view class="goods-list"><block wx:for="{{storelist}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['toStore',['$0'],[[['storelist','',index,'store_id']]]]]]]}}" class="coupon-page s-row" bindtap="__e"><view class="image"><image src="{{item.store_avatar}}"></image></view><view class="content"><view class="title s-row"><text>{{item.store_name}}</text><text class="distance">{{item.distance+" km"}}</text></view><view class="num s-row"><uni-rate disabled="true" size="16" value="{{item.consume_num}}"></uni-rate></view><view class="num s-row"><block wx:if="{{item.voucher!=''}}"><text class="tmprice">{{''+item.voucher}}</text></block></view><view class="money s-row"><text class="zhekou">小易会员享 8.8折</text><text class="zhekou xiaofei">{{item.consume_num+"人消费"}}</text></view></view></view></block></view></block></view><block wx:if="{{storelist.length>0}}"><view class="s-col is-col-24"><load-more loadingType="{{loadingType}}" contentText="{{contentText}}"></load-more></view></block></view>