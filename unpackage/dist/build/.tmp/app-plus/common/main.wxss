.share-mode .wb{width:16%;height:84rpx;background:#d43030 url('https://www.fhlego.com/static/wb_ico.png') center no-repeat;background-size:70%;border-radius:50%;}
.share-bar {width:90%;height:68rpx; padding:20rpx 5%;}
.share-bar .item{width:40%;line-height:68rpx;float:left;}
.share-bar .item text{color:#00BAAD;margin: 0 6rpx;font-weight:600;}
.share-bar .item2{width:60%;line-height:68rpx;float:right;}
.share-list {width:90%;height:56rpx; padding:20rpx 5%;}
.share-list .item{width:60%;line-height:56rpx;}
.share-list .item .icon{width:56rpx;height:56rpx;background: url('https://www.fhlego.com/static/avatar.jpg') center no-repeat;background-size:100%;border-radius:50%;}
.share-list .date{width:30%;line-height:56rpx;font-size:28rpx; color: #999999;}

